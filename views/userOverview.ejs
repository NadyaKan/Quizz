<!DOCTYPE html>
<head>
  <title>User Overview</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="stylesheet" href="/css/useroverview.css" type="text/css" />
</head>
<body>
  <img src="/img/index-bg.jpg" alt="minimalistic shapes" />
  <div class="opacity-black-box"></div>
  <div class="alluser-container">
    <div class="table-container">
      
      <table id="users-table" width="100%">
        <thead>
          <tr>
            <th class="username">USERNAME</th>
            <th class="useremail">EMAIL</th>
            <th class="userpassword">PASSWORD</th>
            <th class="regdate">DATE</th>
          </tr>
        </thead>
        <tr>
          <td class="space"></td>
          <td class="space"></td>
          <td class="space"></td>
          <td class="space"></td>
        </tr>
        <% all.forEach(e => { %>
        <tbody>
          <tr class="row">
            <td>
              <a href="<%=`/user/${e.id}/edit` %>"><%= e.username %></a>
            </td>
            <td><%= e.email_address %></td>
            <td class="passtd"><%= e.user_password %></td>
            <td><%= e.date %></td>
              
          </tr>
        </tbody>
        <% }); %>
      </table>
      
    </div>
  </div>
  
</body>